package model;

public class Toy extends Product {
	private String director;
	private int duration; // Duration in minutes

	public Toy(String id, String name, double price, int quantity, String director, int duration) {
		super(id, name, price, quantity); // Call the constructor of the Product class
		this.director = director;
		this.duration = duration;
	}

	// Getters
	public String getDirector() {
		return director;
	}

	public int getDuration() {
		return duration;
	}

	// Setters
	public void setDirector(String director) {
		this.director = director;
	}

	public void setDuration(int duration) {
		this.duration = duration;
	}

	@Override
	public String toString() {
		return "Toy [ID=" + getId() + ", Name=" + getName() + ", Price=" + getPrice() + ", Director=" + director
				+ ", Duration=" + duration + " mins]";
	}
}
