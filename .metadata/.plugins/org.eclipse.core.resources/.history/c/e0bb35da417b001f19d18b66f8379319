package model;

public class Stationary extends Product {
	private String artist;
	private int trackCount;

	public Stationary(String id, String name, double price, int quantity, String artist, int trackCount) {
		super(id, name, price, quantity); // Call the constructor of the Product class
		this.artist = artist;
		this.trackCount = trackCount;
	}

	// Getters
	public String getArtist() {
		return artist;
	}

	public int getTrackCount() {
		return trackCount;
	}

	// Setters
	public void setArtist(String artist) {
		this.artist = artist;
	}

	public void setTrackCount(int trackCount) {
		this.trackCount = trackCount;
	}

	@Override
	public String toString() {
		return "Stationary [ID=" + getId() + ", Name=" + getName() + ", Price=" + getPrice() + ", Artist=" + artist
				+ ", Track Count=" + trackCount + "]";
	}
}
